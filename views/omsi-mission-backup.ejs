    // determining whether or not to run fixed-nav animation
    var hasHeroClass = jQuery('#main-navigation').hasClass('site-nav--hero');

    // Debounce function for better onscroll performance
    function debounce(func, wait, immediate) {
      var timeout;
      return function() {
        var context = this, args = arguments;
        var later = function() {
          timeout = null;
          if (!immediate) func.apply(context, args);
        };
        var callNow = immediate && !timeout;
        clearTimeout(timeout);
        timeout = setTimeout(later, wait);
        if (callNow) func.apply(context, args);
      };
    };

    var fixedNav = debounce(function() {
      var scrollPos = supportPageOffset ? window.pageYOffset : isCSS1Compat ? document.documentElement.scrollTop : document.body.scrollTop;

      if (scrollPos > navHeight && hasHeroClass) {
        var headerTop = "0px";
        if (isLoggedIn) {
          headerTop = "40px";
        }
        jQuery('#main-navigation').removeClass('site-nav--hero').css('top', -navHeight).addClass('navbar-fixed-top').animate({
          top: headerTop
        }, 100);
